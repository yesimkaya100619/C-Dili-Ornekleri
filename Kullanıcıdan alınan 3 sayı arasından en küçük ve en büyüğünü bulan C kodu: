#include <stdio.h>
#include <stdlib.h>

/* Kullanıcıdan alınan 3 sayı arasından en küçük ve en büyüğünü bulan C kodu: */

int main() {
	
	int s1,s2,s3,kucuk,buyuk,i;
	for(1<=i<=3){
	printf("Birbirinden farkli uc sayi giriniz:");
	scanf("%d)Sayi Giriniz:",&i);
	i++; 	
    }
	if(s1<s2<s3){
		kucuk=s1
		printf("%d en kucuk sayidir\n",kucuk);
	}
	buyuk=s3;
	printf("%d en buyuk sayidir",buyuk);
	

	
	return 0;
}
